{
    "ignore": [
      "README.md"
    ],
    "commands": {
      "Compile contracts": "truffle compile",
      "Migrate contracts": "truffle migrate",
      "Test contracts": "truffle test",
      "Test with reporter": "truffle test --reporter eth-gas-reporter",
      "Test coverage": "truffle run coverage"
    },
    "hooks": {
      "post-unpack": "npm install && cp ../.env .env && chmod +x depVer.sh"
    }
  }